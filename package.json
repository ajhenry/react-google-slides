    "@types/enzyme": "^3.10.5",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "enzyme": "^3.11.0",
    "gh-pages": "^2.1.1",
    "jest": "^25.2.7",
    "prettier": "^1.19.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
